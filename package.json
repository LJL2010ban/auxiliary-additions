{
  "name": "auxiliary-additions",
  "version": "0.0.2",
  "description": "辅助，补充underscore（工具函数，类扩展），兼容IE8+",
  "main": "./build/auxiliary.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --colors ./configs/test/**/*.test.js",
    "start": "browser-sync start --server --files examples/**/*.*",
    "dev": "NODE_ENV=dev webpack --colors --config ./configs/webpack.config.js",
    "product": "NODE_ENV=product webpack --colors --config ./configs/webpack.config.js",
    "release": "npm run dev && npm run product"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapling-team/auxiliary-additions.git"
  },
  "keywords": [
    "tools",
    "extend",
    "fn",
    "class",
    "modules"
  ],
  "author": "icepy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapling-team/auxiliary-additions/issues"
  },
  "homepage": "https://github.com/sapling-team/auxiliary-additions#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "browser-sync": "^2.11.2",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-loader": "^1.3.0",
    "mocha": "^2.4.5",
    "webpack": "^1.12.14"
  }
}
